Int = %Int
Real = %Real
Type = %TyKind

def (+) (x:Real) (y:Real) : Real = %fadd x y

def iadd (x:Int) (y:Int) : Int = %iadd x y

:p iadd 3 (iadd 4 5)
> 12

def depId (a:Type) (x:a) : a = x

:p depId Int 1
> 1

def returnFirstArg (a:Type) (b:Type) (x:a) (y:b) : a = x

:p returnFirstArg Int Real 1 2.0
> 1

:p 1.0 + 2.0
> 3.0

def triple (x:Real) : Real = x + x + x

:p triple 1.0
> 3.0

def idExplicit (a:Type) (x:a) : a = x

:p idExplicit Int 1
> 1

def idImplicit {a:Type} (x:a) : a = x
:p idImplicit 1
> 1

def idImplicit2 : ({a:Type} -> a -> a) = \x. x
:p idImplicit2 1
> 1

:p
  idImplicit3 : ({a:Type} -> a -> a) = \x. x
  idImplicit3 1
> 1

:p (\x y. x + y) 1.0 2.0
> 3.0

:p 1.0 + 1
> Type error:
> Expected: Real
>   Actual: Int
>
> :p 1.0 + 1
>          ^

:p iadd 1 (1.0 + 2.0)
> Type error:
> Expected: Int
>   Actual: Real
>
> :p iadd 1 (1.0 + 2.0)
>            ^^^^^^^^^

:p 1.0 + (iadd 2 3)
> Type error:
> Expected: Real
>   Actual: Int
>
> :p 1.0 + (iadd 2 3)
>           ^^^^^^^^
