
data IntReal =
  MkIntReal Int Real

:p
  case MkIntReal 1 2.3
    MkIntReal x y -> (x, y)
> (1, 2.3)

data MyPair a:Type b:Type =
   MkMyPair a b

z = MkMyPair 1 2.3

:p z
> (MkMyPair 1 2.3)

:t z
> (MyPair Int Real)

:p
  case z
    MkMyPair x y -> (x, y)
> (1, 2.3)

data Dual a:Type =
   MkDual a a

:p
  d = MkDual 1 2
  case d
    MkDual x y -> (x, y)
> (1, 2)
